@model List<IEnumerable<org.buraktamturk.web.Models.revision>>
<?xml version="1.0" encoding="UTF-8" ?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>Burak Tamturk's offical blog</title>
	<link href="http://buraktamturk.org/" />
	<copyright>Copyright 2015 Burak Tamt√ºrk</copyright>
	<language>en-us</language>
	<updated>@Model.First().Last().post.created_at.ToString("yyyy-mm-ddThh:mm:ssZ")</updated>
	@foreach(var post in Model) {
	<entry>
		<title>@post.Last().title</title>
		<link href="http://buraktamturk.org/@(post.Last().path).html" />
		<summary>@Html.Raw(@post.Last().summary())</summary>
		<published>@post.First().created_at.ToString("yyyy-mm-ddThh:mm:ssZ")</published>
		<updated>@post.Last().created_at.ToString("yyyy-mm-ddThh:mm:ssZ")</updated>
		<author>
			<name>@Html.Raw(post.Last().author.name)</name>
			<email>@post.Last().author.mail</email>
		</author>
	</entry>
	}
</feed>
